{
    "ID": "http-kibana-example",
    "Name": "Example Kibana check",
    "Type": "http",
    "Definition": [
        {
            "Host": "{{.Host}}",
            "Path": "/login",
            "HTTPS": true,
            "Port": 5601,
            "Method": "POST",
            "Headers": {
                "Content-Type": "application/json;charset=utf-8"
            },
            "Body": "{\"username\": \"{{.Username}}\", \"password\": \"{{.Password}}\"}",
            "MatchCode": true,
            "code": 204
        },
        {
            "Host": "{{.Host}}",
            "Path": "/app/kibana",
            "HTTPS": true,
            "Port": 5601,
            "Method": "GET",
            "MatchCode": true,
            "Code": 200,
            "MatchContent": true,
            "ContentRegex": "*<script src=\"/bundles/app/kibana/bootstrap\\.js\">"
        }
    ]
}
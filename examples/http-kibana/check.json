{
    "id": "http-kibana-example",
    "name": "Example Kibana check",
    "type": "http",
    "definition": [
        {
            "host": "{{.Host}}",
            "path": "/login",
            "https": true,
            "port": 5601,
            "method": "POST",
            "headers": {
                "Content-Type": "application/json;charset=utf-8"
            },
            "body": "{\"username\": \"{{.Username}}\", \"password\": \"{{.Password}}\"}",
            "matchcode": true,
            "code": 204
        },
        {
            "host": "{{.Host}}",
            "path": "/app/kibana",
            "https": true,
            "port": 5601,
            "method": "GET",
            "matchcode": true,
            "code": 200,
            "matchcontent": true,
            "contentregex": "*<script src=\"/bundles/app/kibana/bootstrap\\.js\">"
        }
    ]
}
{
    "id": "kibana-anarchaos",
    "name": "anarchaos - Kibana",
    "type": "http",
    "group": "${TEAM}",
    "score_weight": 1,
    "definition": {
        "requests": [
            {
                "host": "{{.Host}}",
                "path": "/api/status",
                "https": true,
                "port": 5601,
                "method": "GET",
                "matchcode": true,
                "matchcontent": true,
                "contentregex": "},\"status\":{\"overall\":{\"state\":\"green\","
            },
            {
                "host": "{{.Host}}",
                "path": "/app/kibana",
                "https": true,
                "port": 5601,
                "method": "GET",
                "matchcode": true,
                "matchcontent": true,
                "contentregex": "<script src=\"/bundles/app/kibana/bootstrap\\.js\">"
            }
        ]
    }
}
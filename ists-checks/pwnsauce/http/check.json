{
    "id": "http-pwnsauce",
    "name": "pwnsauce - HTTP",
    "type": "http",
    "group": "${TEAM}",
    "score_weight": 1,
    "definition": {
        "requests": [{
            "host": "pwnsauce.${TEAM}.ists.io",
            "path": "/upload",
            "port": 8080,
            "method": "POST",
            "headers": {
                "Content-Type": "multipart/form-data;boundary=-XXX"
            },
            "body": "---XXX\r\nContent-Disposition: form-data; name=\"type\"\r\n\r\nbash\r\n---XXX\r\nContent-Disposition: form-data; name=\"warez\"; filename=\"test.sh\"\r\nContent-Type: application/x-shellscript\r\n\r\n#!/bin/bash\necho 'oof ouch owie my webshell'\r\n---XXXX--\r\n",
            "matchcode": true,
            "matchcontent": true,
            "contentregex": "/results/[0-9a-f]*",
            "storevalue": true
        },
        {
            "host": "pwnsauce.${TEAM}.ists.io",
            "path": "{{.SavedValue}}",
            "port": 8080,
            "method": "GET",
            "matchcode": true,
            "matchcontent": true,
            "contentregex": "<h2>Standard out:</h1>\n<pre>\noof ouch owie my webshell\n\n</pre>"
        }]
        }
}